{
  "name": "mercurial",
  "version": "1.0.0",
  "description": "Completion candidates from mercurial output (branches)",
  "scripts": {},
  "engines": {
    "coc": ">=0.0.57"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balta2ar/coc-mercurial"
  },
  "keywords": [
    "coc.nvim",
    "mercurial"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "coc.source.mercurial.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.mercurial.shortcut": {
          "type": "string",
          "default": "hg"
        },
        "coc.source.mercurial.disableSyntaxes": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "coc.source.mercurial.priority": {
          "type": "integer",
          "default": 0
        },
        "coc.source.mercurial.filetypes": {
          "type": [
            "array",
            "null"
          ],
          "default": null,
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "author": "baltazar.bz@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/balta2ar/coc-mercurial/issues"
  },
  "homepage": "https://github.com/balta2ar/coc-mercurial",
  "devDependencies": {
    "coc.nvim": ">=0.0.57"
  },
  "dependencies": {
    "which": "^1.3.1"
  },
  "gitHead": "d140659e860e577335353837f0522aeb04a4670f"
}
